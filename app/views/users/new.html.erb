<h1>Signup!</h1>

<%= form_for :user, url: '/users' do |f| %>

  First Name: <%= f.text_field :first_name, required: true %>
  Last Name: <%= f.text_field :last_name, required: true %>
  Email: <%= f.text_field :email, required: true, type: "email" %>
  Password: <%= f.password_field :password, required: true %>
  Password Confirmation: <%= f.password_field :password_confirmation, required: true %>
  <%= f.submit "Submit" %>

<% end %>